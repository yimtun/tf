{
    "ami_id": "ami-0c55b159cbfafe1f0",
    "instance_type": "t2.micro",
    "tags": {
        "Name": "example-instance",
        "Environment": "dev"
    },
    "instances": [
        {
            "ami_id": "ami-0c55b159cbfafe1f0",
            "instance_type": "t2.micro",
            "tags": {
                "Name": "instance-1",
                "Environment": "dev"
            }
        },
        {
            "ami_id": "ami-0c55b159cbfafe1f0",
            "instance_type": "t2.small",
            "tags": {
                "Name": "instance-2",
                "Environment": "prod"
            }
        }
    ],
    "security_group": {
        "name": "example-sg",
        "description": "Example security group",
        "ingress_rules": [
            {
                "from_port": 80,
                "to_port": 80,
                "protocol": "tcp",
                "cidr_blocks": [
                    "0.0.0.0/0"
                ]
            },
            {
                "from_port": 443,
                "to_port": 443,
                "protocol": "tcp",
                "cidr_blocks": [
                    "0.0.0.0/0"
                ]
            }
        ]
    },
    "dns_records": [
        {
            "zone_id": "Z1234567890",
            "name": "example.com",
            "type": "A",
            "ttl": 300,
            "records": [
                "192.0.2.1"
            ]
        },
        {
            "zone_id": "Z1234567890",
            "name": "www.example.com",
            "type": "CNAME",
            "ttl": 300,
            "records": [
                "example.com"
            ]
        }
    ],
    "asg": {
        "name": "example-asg",
        "desired_capacity": 2,
        "max_size": 4,
        "min_size": 1,
        "launch_template": {
            "id": "lt-1234567890",
            "version": "$Latest"
        },
        "tags": {
            "Name": "example-asg",
            "Environment": "dev"
        }
    },
    "parameters": {
        "db_password": {
            "name": "/prod/db/password",
            "description": "Database password",
            "type": "SecureString",
            "value": "secret-password",
            "overwrite": true
        },
        "api_key": {
            "name": "/prod/api/key",
            "description": "API key",
            "type": "SecureString",
            "value": "secret-key",
            "overwrite": true
        }
    },
    "vpc": {
        "cidr_block": "10.0.0.0/16",
        "enable_dns_hostnames": true,
        "enable_dns_support": true,
        "tags": {
            "Name": "example-vpc",
            "Environment": "dev"
        },
        "subnets": [
            {
                "cidr_block": "10.0.1.0/24",
                "availability_zone": "us-west-2a",
                "tags": {
                    "Name": "subnet-1",
                    "Environment": "dev"
                }
            },
            {
                "cidr_block": "10.0.2.0/24",
                "availability_zone": "us-west-2b",
                "tags": {
                    "Name": "subnet-2",
                    "Environment": "dev"
                }
            }
        ]
    }
